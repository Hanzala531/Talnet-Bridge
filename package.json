{
  "name": "talent-bridge",
  "version": "1.0.0",
  "description": "This is basically a plattform designed that provides authentic learning material to student and solves the problem of talent hunting for industries ",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "dev": "nodemon -r dotenv/config --experimental-json-modules ./src/server.js"
  },
  "mocha": {
    "extension": [
      "js"
    ],
    "spec": [
      "tests/integration/**/*.js",
      "tests/unit/**/*.js"
    ],
    "recursive": true,
    "timeout": 10000,
    "exit": true
  },
  "author": "hanzalaTahir",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/vision": "^5.3.3",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.7",
    "cookieparser": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "mongoose": "^8.17.1",
    "multer": "^2.0.2",
    "nodemon": "^3.1.10",
    "rate-limit-redis": "^4.2.1",
    "redis": "^5.8.1",
    "socket.io": "^4.8.1",
    "stripe": "^18.4.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "chai": "^5.3.1",
    "jest": "^29.7.0",
    "mocha": "^11.7.1",
    "mongodb-memory-server": "^10.2.0",
    "nyc": "^17.1.0",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2"
  }
}
